<event name="COMBAT_CHECK_MATH_SHOTGUN_TOGGLE_ADDER">
	<text>What state do you want to set the weapon to?
	Current mode: 'Adder' [12 shots, +1 second cooldown]</text>
	<choice hidden="true">
		<text>Set the weapon to 'Greater Than' Mode. [5 shots, 1 ion/system damage, status+debuff]</text>
		<event>
			<text>You start calibrating the weapon...</text>
			<remove name="MATH_SHOTGUN_TOGGLE_ADDER"/>
			<choice hidden="true">
				<text>Continue...</text>
				<event>
					<text>You finish calibrating the weapon successfully.</text>
					<weapon name="MATH_SHOTGUN_TOGGLE_GREATER_THAN"/>
					<choice hidden="true">
						<text>Continue...</text>
						<event load="COMBAT_CHECK_TOGGLE_LOAD"/>
					</choice>
				</event>
			</choice>
		</event>
	</choice>
	<choice hidden="true">
		<text>Set the weapon to 'Multiplier' Mode. [7 shots, chains cooldown]</text>
		<event>
			<text>You start calibrating the weapon...</text>
			<remove name="MATH_SHOTGUN_TOGGLE_ADDER"/>
			<choice hidden="true">
				<text>Continue...</text>
				<event>
					<text>You finish calibrating the weapon successfully.</text>
					<weapon name="MATH_SHOTGUN_TOGGLE_MULTIPLIER"/>
					<choice hidden="true">
						<text>Continue...</text>
						<event load="COMBAT_CHECK_TOGGLE_LOAD"/>
					</choice>
				</event>
			</choice>
		</event>
	</choice>
	<choice hidden="true">
		<text>Return to the toggle menu.</text>
		<event load="COMBAT_CHECK_TOGGLE_LOAD"/>
	</choice>
</event>

